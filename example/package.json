{
  "name": "serverless-plugin-flambe-example",
  "version": "1.0.0",
  "description": "Showing how serverless-plugin-flambe can be used",
  "scripts": {
    "sls": "npx serverless",
    "build": "rm -rf node_modules/serverless-plugin-flambe && yarn add -D ../",
    "local:hello": "serverless invoke local --function hello -p fixtures/flambe.json",
    "local:goodbye": "serverless invoke local -f goodbye",
    "debug": "serverless flambe"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.0",
    "babel-loader": "^8.0.4",
    "serverless": "^1.35.0",
    "serverless-webpack": "^5.2.0",
    "webpack": "^4.28.0",
    "webpack-node-externals": "^1.7.2"
  }
}
